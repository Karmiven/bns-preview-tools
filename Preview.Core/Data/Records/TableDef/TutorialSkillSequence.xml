<?xml version="1.0" encoding="utf-8"?>
<table name="tutorialskillsequence" version="0.7" autokey="true" module="4">
  <el name="table" child="1" />
  <el name="tutorialskillsequence">
    <record name="alias" type="String" required="y" max="50" />
  </el>
  <el name="step">
    <record name="sequence-step-type" type="Seq" required="y">
      <case name="skill" />
      <case name="effect" />
    </record>
    <record name="skill" type="Int32" default="0" repeat="20" />
    <record name="check-parent-skill" type="Bool" />
    <record name="result" type="Seq" repeat="2">
      <case name="none" default="y" />
      <case name="hit" />
      <case name="miss" />
      <case name="dodge" />
      <case name="parry" />
      <case name="perfect-parry" />
      <case name="bounce" />
      <case name="counter" />
      <case name="critical-hit" />
      <case name="hit-critical-hit" />
      <case name="back-hit-critical-hit" />
      <case name="not-hit" />
      <case name="all" />
      <case name="hit-critical-hit-parry" />
      <case name="parry-perfect-parry" />
      <case name="front-hit-critical-hit" />
      <case name="parry-perfect-parry-counter" />
      <case name="parry-perfect-parry-dodge" />
      <case name="parry-dodge" />
    </record>
    <record name="result-event-type" type="Seq" repeat="2">
      <case name="none" default="y" />
      <case name="attack" />
      <case name="attacked" />
    </record>
    <record name="caster-fail-state" type="Seq16" seq="flag" default="none" />
    <record name="caster-fail-effect" type="Ref" ref="effect" />
    <record name="caster-pass-state" type="Seq16" seq="flag" default="none" />
    <record name="target-pass-state" type="Seq16" seq="flag" default="none" />
    <record name="target-pass-state-count" type="Int8" default="1" />
    <record name="target-pass-effect" type="Ref" ref="effect" />
    <record name="target-pass-effect-count" type="Int8" default="1" />
    <record name="check-only-cast-skill" type="Bool" />
  </el>
</table>