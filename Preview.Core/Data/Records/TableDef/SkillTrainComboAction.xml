<?xml version="1.0" encoding="utf-8"?>
<table name="skill-train-combo-action" version="0.2" autokey="false" module="4">
  <el name="table" child="1" />
  <el name="record">
    <record name="alias" type="String" required="y" max="100" />
    <record name="job" type="Prop_seq" seq="job" key="y" required="y" />
    <record name="job-style" type="Seq" seq="job-style" key="y" required="y" />
    <record name="stance-index" type="Int8" key="y" required="y" min="0" max="255" />
    <record name="buff-index" type="Int8" key="y" required="y" min="0" max="255" />
    <record name="function-index" type="Int8" key="y" required="y" min="0" max="255" />
    <record name="stance-name" type="Ref" ref="text" />
    <record name="buff-name" type="Ref" ref="text" />
    <record name="function-name" type="Ref" ref="text" />
    <record name="function-desc" type="Ref" ref="text" />
    <record name="combo-skill" type="Seq" repeat="10">
      <case name="red" default="y" />
      <case name="blue" />
      <case name="green" />
      <case name="yellow" />
    </record>
    <record name="skill-id" type="Int32" default="0" repeat="10" />
    <record name="variation-id" type="Int8" default="0" repeat="10" />
    <record name="condition-icon-normal-text" type="Ref" ref="text" repeat="10" />
    <record name="condition-icon-over-text" type="Ref" ref="text" repeat="10" />
    <record name="condition-tooltip-text" type="Ref" ref="text" repeat="10" />
  </el>
</table>