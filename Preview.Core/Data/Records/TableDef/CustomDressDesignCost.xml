    <record name="resource-index" type="Int8" key="y" required="y" min="0" />
    <record name="icon-texture" type="Ref" ref="icontexture" />
    <record name="icon-index" type="Int16" default="0" />
    <record name="name" type="Ref" ref="text" />
    <record name="desc" type="Ref" ref="text" />
    <record name="order" type="Int32" required="y" />
    <record name="usable" type="Bool" default="y" />
    <record name="reset-design" type="Bool" />
    <record name="required-item" type="Ref" ref="item" repeat="3" />
