<?xml version="1.0" encoding="utf-8"?>
<table name="guild-craft-recipe" version="0.3" autokey="false" module="6">
  <el name="table" child="1" />
  <el name="record">
    <record name="id" type="Int32" key="y" required="y" />
    <record name="alias" type="String" required="y" max="50" />
    <record name="craft-type" type="Seq" required="y">
      <case name="none" />
      <case name="small" />
      <case name="big" />
    </record>
    <record name="required-guild-level" type="Int8" required="y" />
    <record name="required-faction" type="Ref" ref="faction" />
    <record name="production-type" type="Seq" seq="production-type" default="none" repeat="7" />
    <record name="production-type-required-exp" type="Int16" default="0" repeat="7" min="0" />
    <record name="production-type-required-pc-count" type="Int8" default="0" repeat="7" min="0" />
    <record name="contract-money" type="Int32" default="0" min="0" />
    <record name="contract-item" type="Ref" ref="item" repeat="5" />
    <record name="contract-item-count" type="Int16" default="0" repeat="5" />
    <record name="contract-bm-item" type="Ref" ref="item" />
    <record name="contract-bm-item-count" type="Int16" default="0" min="0" />
    <record name="rent-bm-item" type="Ref" ref="item" />
    <record name="rent-bm-item-count" type="Int16" default="0" min="0" />
    <record name="material" type="Ref" ref="item" repeat="15" />
    <record name="material-count" type="Int16" default="0" repeat="15" min="0" />
    <record name="material-donation-point" type="Int16" default="0" repeat="15" min="0" />
    <record name="brand-material" type="Ref" ref="item-brand" repeat="3" />
    <record name="brand-material-condition-type" type="Seq" seq="condition-type" default="all" repeat="3" />
    <record name="brand-material-count" type="Int16" default="0" repeat="3" min="0" />
    <record name="brand-material-donation-point" type="Int16" default="0" repeat="3" min="0" />
    <record name="time-cost" type="Int32" required="y" />
    <record name="result" type="Ref" ref="item" />
    <record name="result-count" type="Int16" default="0" min="0" />
    <record name="result-bonus-guildlevel" type="Int8" default="0" />
    <record name="result-bonus-count" type="Int16" default="0" />
    <record name="result-receive-type" type="Seq">
      <case name="authorized" default="y" />
      <case name="specified" />
    </record>
    <record name="use-state" type="Seq">
      <case name="none" />
      <case name="no-more-use" />
      <case name="in-use" default="y" />
    </record>
  </el>
</table>