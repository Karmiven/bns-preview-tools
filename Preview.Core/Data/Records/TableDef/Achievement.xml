<?xml version="1.0" encoding="utf-8"?>
<table name="achievement" version="1.1" autokey="false" module="262" maxid="11000">
  <el name="table" child="1" />
  <el name="record">
    <record name="alias" type="String" required="y" max="70" />
    <record name="id" type="Int16" key="y" required="y" min="1" max="11000" />
    <record name="step" type="Int16" key="y" required="y" min="1" max="8" />
    <record name="job" type="Prop_seq" seq="job" key="y" required="y" />
    <record name="deprecated" type="Bool" />
    <record name="complete-from-client" type="Bool" />
    <record name="register-ref" type="Ref" ref="achievement-register" repeat="5" />
    <record name="register-type" type="Seq" repeat="5">
      <case name="none" default="y" />
      <case name="above" />
      <case name="below" />
      <case name="bits-on" />
    </record>
    <record name="register-value" type="Int32" default="0" repeat="5" />
    <record name="progress-show" type="Bool" />
    <record name="step-complete-reward-item" type="Ref" ref="item" repeat="5" />
    <record name="step-complete-reward-item-count" type="Int16" default="0" repeat="5" />
    <record name="step-complete-reward-game-cash" type="Int64" default="0" />
    <record name="step-complete-reward-skill-build-up-point" type="Int16" default="0" />
    <record name="step-complete-reward-type" type="Seq" hidden="y">
      <case name="invalid" />
      <case name="item" />
      <case name="game-cash" />
      <case name="skill-build-up-point" />
    </record>
    <record name="step-complete-reward-final-item" type="Ref" ref="item" repeat="5" hidden="y" />
    <record name="step-complete-reward-final-item-count" type="Int16" default="0" repeat="5" hidden="y" />
    <record name="current-step-score" type="Int16" default="0" />
    <record name="ability" type="Seq">
      <case name="none" default="y" />
      <case name="attack-power-creature-min-max" />
      <case name="attack-hit-value" />
      <case name="attack-pierce-value" />
      <case name="attack-damage-modify-diff" />
      <case name="max-hp" />
      <case name="defend-power-creature-value" />
      <case name="defend-dodge-value" />
      <case name="defend-parry-value" />
      <case name="defend-damage-modify-diff" />
    </record>
    <record name="ability-value" type="Int16" default="0" min="0" max="30000" />
    <record name="completed-effect" type="Ref" ref="effect" />
    <record name="completed-effect-category" type="Seq">
      <case name="0" default="y" />
      <case name="1" />
      <case name="2" />
      <case name="3" />
      <case name="4" />
      <case name="5" />
      <case name="6" />
    </record>
    <record name="completed-effect-order" type="Int16" default="0" min="0" />
    <record name="title-fontset" type="String" max="100" />
    <record name="title-background-image" type="String" max="100" />
    <record name="title-background-image-pos-x" type="Float32" default="0" />
    <record name="title-background-image-pos-y" type="Float32" default="0" />
    <record name="category-1" type="Seq">
      <case name="none" default="y" />
      <case name="growth" />
      <case name="item" />
      <case name="combat" />
      <case name="economy" />
      <case name="community" />
    </record>
    <record name="category-2" type="Seq">
      <case name="none" default="y" />
      <case name="level-up" />
      <case name="quest" />
      <case name="acquire-skill" />
      <case name="consumable" />
      <case name="collect" />
      <case name="growth" />
      <case name="decompose" />
      <case name="repair" />
      <case name="equip-gem" />
      <case name="attach-gem" />
      <case name="dungeon" />
      <case name="faction" />
      <case name="etc" />
      <case name="auction" />
      <case name="production" />
      <case name="social-action" />
      <case name="picture" />
      <case name="event" />
    </record>
    <record name="map-group-1" type="Ref" ref="map-group-1" />
    <record name="icon" type="Icon" max="32767" />
    <record name="name2" type="Ref" ref="text" />
    <record name="description2" type="Ref" ref="text" />
    <record name="title-name" type="Ref" ref="text" />
    <record name="title-image-text" type="Ref" ref="text" />
    <record name="title-thumbnail-icon-text" type="Ref" ref="text" />
    <record name="sort-no" type="Int16" default="0" />
    <record name="completed-game-message" type="Ref" ref="game-message" />
    <record name="talk-social" type="Ref" ref="talksocial" />
    <record name="title-chat-ui-icon" type="Icon" max="32767" />
    <record name="title-thumbnail-frame-fx" type="XUnknown2" />
    <record name="title-grade" type="Int8" default="0" min="0" />
  </el>
</table>