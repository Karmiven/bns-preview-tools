<?xml version="1.0" encoding="utf-8"?>
<table name="treasure-board-page" version="1.0" autokey="true" module="6">
  <el name="table" child="1" />
  <el name="record">
    <record name="alias" type="String" required="y" max="70" />
    <record name="one-time-draw-item" type="Ref" ref="item" />
    <record name="one-time-draw-item-count" type="Int16" default="1" min="1" />
    <record name="five-times-draw-item" type="Ref" ref="item" />
    <record name="five-times-draw-item-count" type="Int16" default="1" min="1" />
    <record name="reset-condition" type="Seq">
      <case name="normal" default="y" />
      <case name="draw-all" />
    </record>
    <record name="reset-price-type" type="Seq">
      <case name="free" default="y" />
      <case name="item" />
    </record>
    <record name="reset-price-item" type="Ref" ref="item" />
    <record name="reset-price-item-count" type="Int16" default="1" min="1" />
    <record name="quota" type="Ref" ref="content-quota" />
    <record name="treasure-board-reward" type="Ref" ref="treasure-board-reward" repeat="59" />
    <record name="treasure-board-name" type="Ref" ref="text" />
    <record name="treasure-board-symbol-on" type="String" max="70" />
    <record name="treasure-board-symbol-off" type="String" max="70" />
  </el>
</table>